{
  "collectCoverage": false,
  "collectCoverageFrom": [
    "src/**/*.{js,jsx}"
  ],
  "coverageThreshold": {
    "global": {
      "lines": 100,
      "branches": 100,
      "functions": 100,
      "statements": 100
    }
  },
  "coverageDirectory": "<rootDir>/coverage/jest",
  "transform": {
    "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
  },
  "transformIgnorePatterns": [
    "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
  ],
  "moduleNameMapper": {
    "\\.(css|scss)$": "identity-obj-proxy"
  },
  "setupFiles": [
    "<rootDir>/config/jest.js"
  ]
}